pipeline{
  agent any
  stages{
      stage ( " SCM Checkout "){
	    steps{
		  checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'Git', url: 'https://github.com/sravanidevi3/Jenkins_Practise.git']]])       
		
		}
	}

   	  stage ( " print satge1 "){
	    steps{
                        dir("${WORKSPACE}/Helloworld/Scripts/")
				   {
				   sh " python Helloworld.py "
				   }

		}
	}

      stage ( " indentation "){
        steps{
		        dir("${WORKSPACE}/Helloworld/Scripts/")
				   {
				   sh " python indent.py "
				   }  

		}
	  }
	  
  }

}
   
		
	  
